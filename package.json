{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description": "E-commerce microservices backend",
  "main": "index.js",
  "scripts": {
    "start:infra": "docker-compose up -d mongodb rabbitmq consul prometheus grafana",
    "start:services": "docker-compose up -d auth-service",
    "start:all": "docker-compose up -d",
    "stop:infra": "docker-compose stop mongodb rabbitmq consul prometheus grafana",
    "stop:services": "docker-compose stop auth-service",
    "stop:all": "docker-compose stop",
    "down:infra": "docker-compose down -v",
    "down:all": "docker-compose down -v",
    "logs:infra": "docker-compose logs -f mongodb rabbitmq consul",
    "logs:auth": "docker-compose logs -f auth-service",
    "logs:all": "docker-compose logs -f",
    "build:auth": "docker-compose build auth-service",
    "rebuild:auth": "docker-compose up -d --build auth-service"
  },
  "keywords": [
    "ecommerce",
    "microservices",
    "nodejs",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {}
}

